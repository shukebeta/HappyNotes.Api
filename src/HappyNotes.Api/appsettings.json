{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedCorsOrigins": [
    "http://localhost:49430",
    "https://happynotes.shukebeta.com",
    "https://happynotes.pages.dev",
    "https://happynotes.today"
  ],
  "Jwt": {
    "SymmetricSecurityKey": "symmetric-security-key-placeholder",
    "Issuer": "https://staging-happynotes-api.dev.shukebeta.com"
  },
  "DatabaseConnectionOptions": {
    "ConnectionString": "server=default_mysql_server; port=6306; uid=default_user; password=password-placeholder; database=HappyNotes; charset=utf8mb4;",
    "DbType":"MySql",
    "ApplicationName": "HappyNotes"
  },
  "ManticoreConnectionOptions": {
    "ConnectionString": "server=default_manticore_server; port=9306; charset=utf8mb4;",
    "DbType":"MySql",
    "HttpEndpoint": "http://default_manticore_server:9312/",
    "ApplicationName": "ManticoreSearch"
  },
  "SeqServerUrl": "https://seq.dev.shukebeta.com",
  "SeqApiKey": "seq-api-key-placeholder",
  "Version": "VERSION_PLACEHOLDER",
  "PollyPolicies": {
    "TelegramRetry": {
      "SleepDurations": [
        "00:00:01",
        "00:00:05",
        "00:00:10"
      ]
    }
  },
  "SyncQueue": {
    "Redis": {
      "ConnectionString": "redis-connection-placeholder",
      "Database": 1,
      "KeyPrefix": "happynotes:sync:"
    },
    "Processing": {
      "MaxConcurrentTasks": 1,
      "ProcessingTimeout": "00:10:00",
      "PollingInterval": "00:00:05",
      "VisibilityTimeout": "00:20:00",
      "RecoveryInterval": "00:15:00"
    },
    "Handlers": {
      "telegram": {
        "maxRetries": 3,
        "baseDelaySeconds": 60,
        "backoffMultiplier": 2.0,
        "maxDelayMinutes": 60
      }
    }
  },
  "AllowedHosts": "*"
}
